{
    "name": "antarctica/laravel-json-exception-formatter",
    "description": "A custom interface for the Laravel JSON Exception Formatter package.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Fennell",
            "email": "felnne@bas.ac.uk"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "radweb/json-exception-formatter": "0.2.*"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/felnne/JSON-Exception-Formatter"
        }
    ],
    "autoload": {
        "psr-4": {
            "Antarctica\\JsonExceptionFormatter\\": "src"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}